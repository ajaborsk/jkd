<?xml version = "1.0"  encoding="UTF-8" standalone="yes"?>
<application name="demo_pi">
  <nodes>
    <serial_capture name="serial1" serial_port="/dev/ttyUSB0" serial_baudrate="57600" />
    <serial_line_process name="serial_parser">
      <links>
        <link node="serial1" port="output" input="input" />
      </links>
    </serial_line_process>
    <history name="jee_histo">
      <links>
        <link input="input" connect="auto" policy="on_update" node="serial_parser" port="output" />
      </links>
    </history>
    <data_process_0 name="data_process_0_0">
      <links>
        <link input="input" node="jee_histo" port="output" />
      </links>
    </data_process_0>
    <subprocessus name="heavyproc" appname="erer">
      <nodes>
        <signal_generator name="siggen_ext" />
        <!-- This is a comment -->
      </nodes>
    </subprocessus>
    <html_page appname="demo_pi" name="homepage" template="homepage" title="A jkd Demo Application">
      <table data="node:/heavyproc#output" template="table" />
      <value data="node:/siggen#output" template="simple_value" />
    </html_page>
  </nodes>
  <ports>
    <port name="index.html" node="homepage" port="html" />
  </ports>
</application>
